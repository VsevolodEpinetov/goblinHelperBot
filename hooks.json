[
    {
      "id": "webhook",
      "execute-command": "/home/server/TelegramGoblinBot/update.sh",
      "command-working-directory": "/home/server/TelegramGoblinBot",
      "pass-arguments-to-command":
      [
        {
          "source": "payload",
          "name": "head_commit.id"
        },
        {
          "source": "payload",
          "name": "pusher.name"
        },
        {
          "source": "payload",
          "name": "pusher.email"
        }
      ],
      "trigger-rule":
      {
        "and":
        [
          {
            "match":
            {
              "type": "payload-hash-sha1",
              "secret": "verysecret",
              "parameter":
              {
                "source": "header",
                "name": "X-Hub-Signature"
              }
            }
          },
          {
            "match":
            {
              "type": "value",
              "value": "refs/heads/master",
              "parameter":
              {
                "source": "payload",
                "name": "ref"
              }
            }
          }
        ]
      }
    }
  ]